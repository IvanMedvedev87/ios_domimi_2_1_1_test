<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.domimi.domimi" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>DomiMi</name>
    <description />
    <content src="index.html" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="pgb-builder-version" value="2" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <icon src="www/icon.png" />
    <splash src="www/splash.png" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="20" src="www/res/icon/ios/Icon-App-20x20@1x.png" width="20" />
        <icon height="48" src="www/res/icon/ios/Icon-24@2x.png" width="48" />
        <icon height="55" src="www/res/icon/ios/Icon-27.5@2x.png" width="55" />
        <icon height="58" src="www/res/icon/ios/Icon-29@2x.png" width="58" />
        <icon height="87" src="www/res/icon/ios/Icon-29@3x.png" width="87" />
        <icon height="80" src="www/res/icon/ios/Icon-40@2x.png" width="80" />
        <icon height="88" src="www/res/icon/ios/Icon-44@2x.png" width="88" />
        <icon height="172" src="www/res/icon/ios/Icon-86@2x.png" width="172" />
        <icon height="196" src="www/res/icon/ios/Icon-98@2x.png" width="196" />
        <icon height="40" src="www/res/icon/ios/Icon-App-20x20@2x.png" width="40" />
        <icon height="60" src="www/res/icon/ios/Icon-App-20x20@3x.png" width="60" />
        <icon height="29" src="www/res/icon/ios/Icon-App-29x29@1x.png" width="29" />
        <icon height="40" src="www/res/icon/ios/Icon-App-40x40@1x.png" width="40" />
        <icon height="120" src="www/res/icon/ios/Icon-App-40x40@3x.png" width="120" />
        <icon height="180" src="www/res/icon/ios/Icon-App-60x60@3x.png" width="180" />
        <icon height="76" src="www/res/icon/ios/Icon-App-76x76@1x.png" width="76" />
        <icon height="152" src="www/res/icon/ios/Icon-App-76x76@2x.png" width="152" />
        <icon height="167" src="www/res/icon/ios/Icon-App-83.5x83.5@2x.png" width="167" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <engine name="ios" spec="5.1.0" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~3.0.0" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.4.2" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="~5.0.3" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.3.3" />
</widget>